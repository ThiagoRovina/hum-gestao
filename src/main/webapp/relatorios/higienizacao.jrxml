	<parameter name="unidadeHigienizacao" isForPrompting="false" class="java.lang.String"/>
	<parameter name="dtInicioHigienizacao" isForPrompting="false" class="java.util.Date"/>
	<parameter name="dtFimHigienizacao" isForPrompting="false" class="java.util.Date"/>
	<queryString><![CDATA[SELECT 
            BHH.CD_HISTORICO_HIGIENIZACAO,
            BHH.DH_SOLICITACAO,
            BHH.DH_INICIO,
            BHH.DH_FIM,
            CASE BHH.TP_HIGIENIZACAO
                WHEN 0 THEN 'Simples'
                WHEN 1 THEN 'MR'
                WHEN 2 THEN 'MR Preto'
                ELSE 'Desconhecido'
            END as TP_HIGIENIZACAO,
            CASE BU.ST_LEITO
                WHEN 0 THEN 'Livre'
                WHEN 1 THEN 'Ocupado'
                WHEN 2 THEN 'Em Higienização'
                WHEN 3 THEN 'Pré-Reservado'
                WHEN 4 THEN 'Aguardando Liberação'
                WHEN 5 THEN 'Desativado'
                ELSE 'Desconhecido'
            END as ST_LEITO,
            BU2.DE_UNIDADE,
	    BU.ID_LEITO 
        FROM HUM.BDC_HISTORICO_HIGIENIZACAO bhh
        INNER JOIN HUM.BDC_LEITO bu 
            ON BHH.CD_LEITO = BU.CD_LEITO 
        LEFT JOIN HUM.BDC_UNIDADE bu2 
            ON bu2.CD_UNIDADE = BU.CD_UNIDADE
	WHERE BHH.DH_INICIO >= $P{dtInicioHigienizacao} AND BHH.DH_FIM <= $P{dtFimHigienizacao}
	    AND BU2.CD_UNIDADE = $P{unidadeHigienizacao}]]></queryString>

